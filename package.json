{
  "name": "webpack-plugin-dev-forbidden-string-checker-plugin",
  "version": "1.0.1",
  "description": "在开发环境使用，查找违禁词 可以用正则。国际化项目可以用来找没有翻译的中文",
  "main": "dits/index",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build:unbuild": "rm -rf dist && unbuild",
    "build": "rm -rf dist && tsc && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "SRongr",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.9.3",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.102.1"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "unbuild": "^3.6.1"
  }
}
